{
    "name": "Distrocord",
    "description": "Distributed Discord Library",
    "license": "MIT",
    "scripts": {
        "build": "webpack --config webpack.config.js",
        "watch": "mkdir -p build && webpack --config webpack.config.js --watch",
        "start": "nodemon -w ./build --harmony build/index.js",
        "dev": "concurrently --kill-others \"yarn run watch\" \"sleep 60; yarn run start\""
    },
    "dependencies": {
        "@types/bluebird": "^3.5.8",
        "@types/es6-promise": "^0.0.33",
        "@types/mongodb": "^2.2.10",
        "@types/mongoose": "^4.7.21",
        "@types/node": "^8.0.25",
        "@types/node_redis": "^0.8.29",
        "@types/redis": "^2.6.0",
        "@types/uws": "^0.13.0",
        "bluebird": "^3.5.0",
        "concurrently": "^3.5.0",
        "erlpack": "^0.1.2",
        "eventemitter3": "^2.0.3",
        "https": "^1.0.0",
        "mongo": "^0.1.0",
        "mongoose": "^4.11.8",
        "nodemon": "^1.10,<1.11",
        "redis": "^2.8.0",
        "source-map-support": "^0.4.16",
        "ts-loader": "^2.3.4",
        "ts-node": "^3.3.0",
        "tslint": "^5.7.0",
        "tslint-loader": "^3.5.3",
        "typegoose": "^3.1.1",
        "typescript": "^2.4.2",
        "uws": "^8.14.1",
        "validator.ts": "^0.2.2",
        "webpack": "^3.5.5",
        "zlib": "^1.0.5"
    }
}
